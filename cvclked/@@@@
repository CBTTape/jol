         INCLUDE OBJ(@@@JOBQ)
         REPLACE UJEJOBQ
         CHANGE  UJEJOBQ(@@@JOBQ),UJE00MN(UJE00SH)
         INCLUDE SYS2($JOLSH35)
         INCLUDE OBJ(UJTNUM)
         CHANGE  UJEJOBQ(OLDJOBQ)
         INCLUDE OBJ(UJEJOBQ)
         PAGE UJE00SH
         ENTRY UJE00SH
         NAME $JOLSH51(R)
