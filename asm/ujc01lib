           TITLE 'PROCESS REGION/SIZE PARAMETER'                        00529000
JBREGION   EQU *                                                        00530000
JBSIZE     EQU *                                                        00531000
            MVC   #WORK(L'TKN),#TKN                               75128 00532000
           ENTRY JBREGION                                               00533000
           ENTRY JBSIZE                                                 00534000
         GETTKN TKNCURR       GET NEXT TOKEN.                     75128 00535000
         CLC   =C'K ',TKN     NNNN K ?                            75128 00536000
         BE    C01SIZEK       YES, K CODED                        75128 00537000
         CLC   =C'BYTES ',TKN BYTES CODED ?                       75128 00538000
         BNE   *+8                                                75128 00539000
         STH   R1,TKNCURR     RESET TOKEN POINTER                 75128 00540000
         TNUM  #WORK          CONVERT TO NUMERIC                  75128 00541000
         LTR   R15,R15        DID IT CONVERT ?                    75128 00542000
         BNZ   C01STRSZ       NOPE, LET JOLSTOR GIVE ERR MESSAGE  75128 00543000
         SR    R0,R0          PREPARE TO DIVIDE                   75128 00544000
         D     R0,=F'1024'    DIVIDE BY 1 K                       75128 00545000
         LTR   R0,R0          ANY REMAINDER ?                     75128 00546000
         BZ    *+8                                                75128 00547000
         LA    R1,1(R1)       ADD 1 TO K NUMBER                   75128 00548000
* NOTE, WE LEAVE ROUNDING UP TO 2K TO OS                          75128 00549000
         CVD   R1,DBL         CONVERT TO DECIMAL                  75128 00550000
         UNPK  WORK(5),DBL    NOW ZONED                           75128 00551000
         OI    WORK+4,C'0'    FIX LAST BYTE                       75128 00552000
         MVI   #WORK+1,5      SET LENGTH OF #WORK TO 5            75128 00553000
         SPACE 1                                                  75128 00554000
C01STRSZ JOLSTOR 'SIZE/REGION',JOBREGN,#WORK,NUM                  75128 00555000
         LTR   R15,R15                                            75128 00556000
         BNZ   A                                                  75128 00557000
         MVI   JOBREGNQ,C'K'  SET UP 'K'                          75128 00558000
         B     A                                                  75128 00559000
         SPACE 1                                                  75128 00560000
C01SIZEK STH   R1,TKNCURR                                               00561000
         B     C01STRSZ                                                 00562000
         TITLE 'PROCESS MESSAGE CLASS'                            75128 00563000
