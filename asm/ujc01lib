           SPACE 3                                                      00056000
* ON ENTRY:-                                                      74256 00057000
* R2 -> JOLCOM                                                    74256 00058000
* R3 -> TKNX                                                      74256 00059000
*                                                                 74256 00060000
           USING TKNX,R3                                                00061000
           USING JOLCOM,R2                                              00062000
         USING C01DSAVE,R13                                             00063000
         L     R4,AJOBDETS                                        75128 00064000
         USING JOBDETS,R4                                         75128 00065000
*       GETMAIN R,LV=C01ACCTE-C01ACCTS                             DASD 00066000
        LA      R1,C01ACCTB                                       84001 00068000
        ST      R1,C01#ACCT       SAVE AREA POINTER                DASD 00067000
        USING   C01ACCTB,R1                                        DASD 00068000
         CLEAR  C01ACCTS  CLEAR JOB ACCOUNT WORK AREA              DASD 00069000
         CLEAR  #C01ACCT,#C01SUB2,#C01SUB3,#C01SUB4,#C01SUB5,          *00070000
               #C01SUB6,#C01SUB7,#C01SUB8                          DASD 00071000
        DROP    R1                                                 DASD 00072000
         CLI   JOBNAME,C' '   JOB CARD CODED ?                          00073000
         BE    C01NOVRD       NOPE, NOT YET                             00074000
