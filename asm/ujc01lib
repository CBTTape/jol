           ENTRY JBSUBMIT,JBNAME                                        00260000
JBPROGRA   EQU *                                                        00261000
           ENTRY JBPROGRA                                               00262000
           MVC #WORK(50),#TKN                                           00263000
TMORENME   GETTKN LOCN=TKNCURR                                          00264000
           CLI TKN,C'.'                        MORE OF THE NAME ?       00265000
           BE  MORENAME                                                 00266000
STORNAME MVC   #TKN(L'TKN+2),#WORK COPY TO TKN           JOL30052 76200 00267000
         SR    R10,R10                                   JOL30052 76200 00268000
         IC    R10,TKN        LOAD QUOTE (IF ANY)        JOL30052 76200 00269000
         BAL   R14,DROPQUOT   DROP QUOTES (IF ANY)       JOL30052 76200 00270000
         JOLSTOR 'PROGRAMMER NAME',JOBPRGMR,#TKN         JOL30052 76200 00271000
         LTR   R15,R15        DID WE STORE IT ?          JOL30052 76200 00272000
         BNZ   A              NOPE                       JOL30052 76200 00273000
         CH    R10,=XL2'007D' WAS IT A QUOTE ?           JOL30052 76200 00274000
         BE    *+8            YES ->                     JOL30052 76200 00275000
         LA    R10,C' '       NO, KEEP R10 AS A BLANK    JOL30052 76200 00276000
         STC   R10,JOBPRGMQ   STORE INDICATOR IN QUOTE INDIC  052 76200 00277000
           B   A                                                        00278000
