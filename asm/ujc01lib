         TITLE 'PROCESS TIME PARAMETER(S)'                              00453000
         ENTRY JBTIME                                                   00454000
JBTIME     MVC #WORK(15),#TKN                  SAVE FIRST TIME          00455000
           GETTKN LOCN=TKNCURR                                          00456000
*                                                        JOL30077 76200 00457000
           CLI TKN,C','                        ARE THE 2 TIMES ?        00458000
           BE  TIME2                                                    00459000
* THERE IS ONLY ONE TIME PARAMETER.                                     00460000
           TNUM #WORK                                                   00461000
           LTR R15,R15                                                  00462000
         BNZ   TIMERR                                             75128 00463000
           JOLSTOR 'TIME',JOBELAP,#WORK                                 00464000
           B   TIMEQUAL                                                 00465000
TIME2    GETTKN REG=R1                                                  00466000
         STH   R1,TKNCURR                                               00467000
         TNUM  #TKN                                                     00468000
         LR    R5,R1          SAVE BINARY NUMBER                  75128 00469000
         LTR   R15,R15        DID IT CONVERT OK ?                 75128 00470000
         BNZ   TIMERR         NO                                  75128 00471000
*                                                                 75128 00472000
         TNUM  #WORK                                                    00473000
         LTR   R15,R15                                                  00474000
         BNZ   TIMERR                                             75128 00475000
         CH    R1,=H'1440'    NO CPU TIMING ?                     75128 00476000
         BE    WORK1440                                                 00477000
         CH    R5,=H'1440'    NO CPU TIMING ?                     75128 00478000
         BE    TKN1440                                                  00479000
* NOW TAKE THE LARGEST TIME AND STORED IT AS THE ELASPED TIME           00480000
         CR    R5,R1                                              75128 00481000
         BH    TKNELAP                         TKN CONTAINS ELAPSED TME 00482000
TKN1440  DS    0H                                                       00483000
         JOLSTOR 'ELAPSED TIME',JOBELAP,#WORK                           00484000
         JOLSTOR 'CPU TIME',JOBCPU,#TKN                                 00485000
           B   TIMEQUAL                                                 00486000
WORK1440   EQU *                                                        00487000
TKNELAP    JOLSTOR 'ELAPSED TIME',JOBELAP,#TKN                          00488000
           JOLSTOR 'CPU TIME',JOBCPU,#WORK                              00489000
TIMEQUAL L     R1,AJOLGEN                                               00490000
         USING GENDETS,R1                                               00491000
         BOX   'THIS AREA MAY NEED MODIFICATION FOR JES ****'     75128 00492000
         CLI   SPOOL,C'H'     HASP ?                              75128 00493000
         BE    C01SETSB       SET UP SUBACCOUNT FIELD             75128 00494000
         CLI   SPOOL,C'2'     JES2 ?                              75128 00495000
         BE    C01SETSB       YES                                 75128 00496000
         DROP  R1                                                       00497000
         B     C01TMQ2                                            75128 00498000
C01SETSB EQU   *                                                  75128 00499000
         MVC   WORK(L'JOBELAP),JOBELAP                            80290 00500000
         MVC   #WORK,=AL2(L'JOBELAP)                              80290 00500005
         BAL   R14,DRPBWORK   DROP ENDING BLANKS FROM WORK        80290 00500010
         L      R15,C01#ACCT   POINT TO WORK AREA                  DASD 00500011
         USING  C01ACCTB,R15                                       DASD 00500012
         IFVALUE #C01SUB3,C01TMQ2  IF SOMEONE HAS VALUE,LEAVE IT  80290 00500015
         MVC   #C01SUB3(L'JOBELAP),#WORK  SHIFT TO SUB ACCT3      80290 00500020
         DROP   R15                                                     00500030
C01TMQ2  GETTKN LOCN=TKNCURR  SEE IF MINUTES ETC                  75128 00501000
         L     R15,=V(UJCTM)  LOAD TIME QUALIFIER TABLE           75128 00502000
         LM    R7,R9,0(R15)                                       75128 00503000
         CLC   TKN(8),0(R7)                                       74256 00504000
         BE    ISTIMEQ                                            74256 00505000
         BXLE  R7,R8,*-10                                         74256 00506000
* TIME NOT QUALIFIED SO GO TO A AND LEAVE THE TIME THE WAY IT WAS.      00507000
           B   A                                                        00508000
ISTIMEQ    MVC JOBTIMEQ,TKN                                             00509000
           STH R1,TKNCURR                                               00510000
           B   A                                                        00511000
TIMERR     EQU   *                                                      00512000
         JOLERR 303,'ERROR IN TIME PARAMETER'                     75128 00513000
           B A                                                          00514000
           TITLE 'PROCESS CLASS PARAMETER'                              00515000
