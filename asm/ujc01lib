           ENTRY JBROLL                                                 00570000
           MVC #TKN,=H'1'                      ONLY STORING 1 CHARACTER 00571000
*                                              AND DON'T WANT LENGTH ER 00572000
           JOLSTOR 'ROLL',JOBROLL1,#TKN        -RROR TO BE THROWN UP AT 00573000
*                                              US                       00574000
           GETTKN LOCN=TKNCURR                                          00575000
           CLI TKN,C','                        2ND 1 CODED ?            00576000
           BNE A                               NO,GET NEXT FIELD        00577000
           STH R1,TKNCURR                      RESET POINTER            00578000
           GETTKN LOCN=TKNCURR                 GET THE 2ND ROLL PARM    00579000
           STH R1,TKNCURR                      POINT TO TOKEN AFTER IT  00580000
           MVC #TKN,=H'1'                      AGAIN,DON'T WANT LENGTH  00581000
           JOLSTOR 'ROLL',JOBROLL2,#TKN        ERROR WHEN STORING       00582000
           B   A                                                        00583000
