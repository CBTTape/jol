*                             FOR THE GENERAL PURPOSE STORE RTN         00076000
         JOLERR 101,'JOB DETAILS MERGED'                          75311 00077000
C01NOVRD   EQU  *                                                       00078000
           SPACE 2                                                      00079000
* NOW STORE THE JOBNAME                                                 00080000
         IFNULL LABEL,C01START                                    75128 00081000
           JOLSTOR 'JOBNAME',JOBNAME,LABEL                              00082000
C01START   MVC TKNCURR,=H'2'                   START PROCESSING AT 2    00083000
A          CLC TKNCURR,TKNNO                   HAVE WE FINISHED ?       00084000
           BNH FINDKEY                         NO,SO FIND A KEY WORD    00085000
*                                              OR SOME SUCH.            00086000
C01RETN0   EQU  *                                                       00087000
RETN0     EQU   *                                                  DASD 00088000
***************************************************************    DASD 00089000
* COPY JOB ACCOUNT FIELDS FROM THE WORK AREA TO JOLS FIXED    *    DASD 00090000
* AREAS IN COMMON                                             *    DASD 00091000
***************************************************03-15-78****    DASD 00092000
         CLI    JOBSUB2,X'00'  DO WE HAVE A QUOTED ACCT='??'       DASD 00093000
         BE     RETNOW                                             DASD 00094000
         L      R15,C01#ACCT   POINT TO WORK AREA                  DASD 00095000
         USING  C01ACCTB,R15                                       DASD 00096000
         LA     R14,JOBACCT    POINT TO JOLS FIXED AREA            DASD 00097000
         LA     R7,220         LENGTH OF JOL AREA                  DASD 00098000
         LA     R8,8           # OF ACCOUNT FIELDS TO MOVE         DASD 00099000
C01ALOOP EQU    *                                                  DASD 00100000
         LH     R9,#C01ACCT    GET LENGTH OF FIELD                 DASD 00101000
         AR     R9,R15         POINT TO END OF FIELD               DASD 00102000
         MVI    2(R9),C','     END OF THISS FIELD                  DASD 00103000
         LH     R1,0(R15)                                         80290 00104000
         EX     R1,C01MVC      MOVE FIELD                         80290 00104005
         LH     R9,#C01ACCT                                        DASD 00106000
         LA     R9,1(R9)                                           DASD 00107000
         SR     R7,R9                                              DASD 00108000
         BM     RETNOW          RETURN IF WE HAVE EXCEEDED JOLS AEADASD 00109000
         AR     R14,R9          POINT TO NEXT AREA                 DASD 00110000
         LA     R15,L'#C01ACCT+L'C01ACCT(R15)  ADJUST TO NEXT AREA DASD 00111000
         BCT    R8,C01ALOOP     RETURN TO NEXT ACCT FIELD          DASD 00112000
          DROP   R15                                               DASD 00113000
*RETNOW    FREEMAIN R,LV=C01ACCTE-C01ACCTS,A=C01#ACCT             DASD  00114000
RETNOW    DS    0H                                                84100 00114000
          JOLRETN    RC=0                                          DASD 00115000
           TITLE 'EXAMINE TOKEN AND BRANCH TO PROCESSING ROUTINE'       00116000
FINDKEY    GETTKN LOCN=TKNCURR                                          00117000
           CLC TKNCURR,TKNNO                                            00118000
           BH  RETN0                           ENDED                    00119000
           STH R1,TKNCURR                                               00120000
           CLI TKN,C','                                                 00121000
           BE  FINDKEY                                                  00122000
         CLI   TKN,C')'       CLOSING BRACKET TO BE IGNORED ?     75128 00123000
         BE    FINDKEY        YES                                 75128 00124000
           SPACE 3                                                      00125000
* FIRST FIND ITEMS WHICH MAY NOT HAVE KEYWORDS                          00126000
         CLEAR C01DBRAC ,      CLEAR OPEN BRACKET INDIC           75128 00127000
         MVC   #WORK(30),#TKN SAVE TOKEN                          75128 00128000
           LR  R6,R1                           KEEP POINTER TO TOKEN    00129000
           BCTR R6,0                           -1 COS GETTKN ADDED 1    00130000
           GETTKN LOCN=TKNCURR                                          00131000
           CLI TKN,C'.'                        PROGRAMMER NAME ?        00132000
           BNE TRYTIME                          NO,TRY SOMETHING ELSE   00133000
           GETTKN REG=R6                                                00134000
           B   JBPROGRA                                                 00135000
           SPACE 3                                                      00136000
TRYTIME    GETTKN REG=R6                       GO BACK 1 TOKEN          00137000
           CLI TKNTYPE,0                       IS IT A NUMBER ?         00138000
           BNE TRYKEYWD                         NO,NOW SCAN THE KEYWORD 00139000
*                                              TABLE                    00140000
           SPACE 3                                                      00141000
* NOW IN TKN WE HAVE A NUMBER                                           00142000
           MVC #WORK(50),#TKN                                           00143000
           GETTKN REG=R1                       GET THE NEXT TOKEN AFTER 00144000
*                                              THE NUMBER.              00145000
         CLC   =C'K',TKN      SIZE NNN K ?                        75128 00146000
           BE  ISSIZE                                                   00147000
           CLC TKN(6),=C'BYTES '                                        00148000
           BE  ISSIZE                                                   00149000
* NOW ALL WE HAVE LEFT TO CHECK IS 'TIME' SO 1ST SEE IF THIS IS A       00150000
*    COMMA AND IF SO GET THE NEXT TKN,CHECK IT TO BE A NUMBER AND IF    00151000
*    SO THE CHECK THE NEXT TKN TO BE'MINUTES' ETC.                      00152000
           CLI TKN,C','                                                 00153000
           BNE TRYTIME2                                                 00154000
           GETTKN REG=R1                                                00155000
           CLI TKNTYPE,0                       NUMBER ?                 00156000
           BNE C01WHAT                                                  00157000
           GETTKN REG=R1                       GET WHAT IS HOPEFULLY    00158000
*                                              NOW THE QUALIFIIFIER     00159000
           SPACE                                                        00160000
TRYTIME2   EQU *                                                        00161000
         L     R15,=V(UJCTM)  GET TIME KEYWORDS, EG MINS          75128 00162000
         LM    R7,R9,0(R15)                                       75128 00163000
C01TRYT3 CLC   TKN(8),0(R7)   IS IT M,MINS,ETC?                   74256 00164000
         BE    ISTIME         YES,OUT WE GO                       74256 00165000
         BXLE  R7,R8,C01TRYT3 LOOP LOOKING FOR MORE               74256 00166000
         GETTKN (R6)          GET ORIGINAL TOKEN AGAIN            75128 00167000
           TITLE 'CALL USER EXIT TO DEAL WITH UNKNOWN TOKEN'            00168000
C01WHAT  L     R15,ACOMPEXI   LOAD USERS EXIT ROUTINES ADDRESS    75128 00169000
         L     R15,0(R15)                                         75128 00170000
         $CALL  (15),(JOBDETS,TKNX)                               75128 00171000
           LTR R15,R15                         DID USER DEAL WITH IT ?  00172000
         BNZ   C01ERR2        ERROR, USER DIDN'T FIX IT           75128 00173000
* WELL USER DID SOMETHING WITH IT, LETS SEE WHAT                  75128 00174000
         CLI   TKN,C' '       DID HE STORE INFO HIMSELF ?         75128 00175000
         BE    A              YES, BACK TO MAIN LINE              75128 00176000
         LTR   R15,R15        DID HE DO ANYTHING?                 75128 00177000
         BNZ   C01ERR2        NOPE                                75128 00178000
         L     R1,=V(UJCJBKEY)                                    75128 00179000
         LM    R7,R9,0(R1)                                        75128 00180000
         CLC   TKN(8),0(R7)                                       75128 00181000
         BE    GOTKEYWD                                           75128 00182000
         BXLE  R7,R8,*-10                                         75128 00183000
           GETTKN REG=R6                                                00184000
         STH   R1,TKNCURR     STORE POINTER NEXT TKN              75128 00185000
         MVC   #WORK(L'TKN+2),#TKN  SAVE TOKEN                    75128 00186000
         GETTKN (R1)          GET NEXT, AND IF '=',CONCAT IT AND  75128 00187000
*                             NEXT                                75128 00188000
         CLI   TKN,C'='       '='                                 75128 00189000
         BNE   C01ERR2                                            75128 00190000
         BAL   R14,CONCAT     CONCAT '='                          75128 00191000
         GETTKN (R1)          GET NEXT ITEM                       75128 00192000
         STH   R1,TKNCURR     RESET POINTER                       75128 00193000
         BAL   R14,CONCAT     CONCAT '='                          75128 00194000
C01ERR2  JOLERR 302,'''',#WORK,''' IGNORED:- UNRECOGNISABLE'      75128 00195000
           B   A                                                        00196000
ISTIME     GETTKN REG=R6                                                00197000
           B   JBTIME                                                   00198000
           SPACE                                                        00199000
ISSIZE     GETTKN (R6)                                                  00200000
           B  JBSIZE                                                    00201000
           TITLE 'EXAMINE KEY-WORK AND PARAMETER TABLES'                00202000
TRYKEYWD   EQU *                                                        00203000
         GETTKN (R6)          GET KEYWORD                         75128 00204000
* IN ACTUAL FACT WE SHALL LOOK FOR 'HOLD' ETC FIRST                     00205000
           L   R1,=V(UJCJBPRM)                 ADDR OF TABLE            00206000
         LM    R14,R15,4(R1)  LOAD INCR AND STOP ADDRESSES        75128 00207000
         L     R1,0(R1)       LOAD START ADDR OF TABLE            75128 00208000
PARMLOOP   CLC TKN(8),0(R1)                                             00209000
           BE  GOTPARM                                                  00210000
           BXLE R1,R14,PARMLOOP                                         00211000
           SPACE 3                                                      00212000
* NOW LOOK FOR KEYWORDS                                                 00213000
           L   R1,=V(UJCJBKEY)                 ADDR TABLE               00214000
         LM    R7,R9,0(R1)    LOAD TABLE BXLE CONS                75128 00215000
         CLC   TKN(8),0(R7)   KEY IN TABLE = ?                    75128 00216000
           BE  GOTKEYWD                                                 00217000
         BXLE  R7,R8,*-10                                         75128 00218000
         B     C01WHAT                                            75128 00219000
           SPACE 3                                                      00220000
GOTPARM    EQU *                                                        00221000
         MVC   DBL,C01LAR15   SHIFT LA R15,0 INSTRUCTION          75128 00222000
         MVC   DBL+2(2),8(R1) SHIFT S CONS                        75128 00223000
         EX    R0,DBL         LA R15,PROCESSING ROUTINE           75128 00224000
           LTR R15,R15                                                  00225000
           BZ  C01NIMPL        NOT YET IMPLEMENTED,MATE                 00226000
           BALR R14,R15                        GO TO PROCESSING ROUTINE 00227000
* BY RIGHTS WE SHOULD NEVER COME BACK HERE BUT IN CASE THE USER HAS     00228000
*          PUT HIS OWN KEYWORD HANDLING ROUTINE IN I'LL STICK A BRANCH  00229000
*          IN                                                           00230000
           B   A                                                        00231000
C01NIMPL JOLERR 304,'''',#TKN,''' NOT YET IMPLEMENTED'            75128 00232000
           B A                                                          00233000
           SPACE 3                                                      00234000
C01ISBRA MVI   C01DBRAC,C'('  SHIFT IN BRACKET INDICATOR          75128 00235000
         STH  R1,TKNCURR                                          75128 00236000
GOTKEYWD   EQU *                                                        00237000
         LR    R10,R7                             SAVE TABLE POSN 75128 00238000
*                                              WE FOUND THE KEY-WORD    00239000
           GETTKN LOCN=TKNCURR                                          00240000
         CLI   TKN,C'('       USER CODED ( ) AROUND ITEM ?        75128 00241000
         BE    C01ISBRA       YES, SAVE '(' IND FOR LATER USE     75128 00242000
           CLI TKN,C'='                                                 00243000
           BNE GOTTOKEY                                                 00244000
           GETTKN REG=R1                                                00245000
         CLI   TKN,C'('                                           75128 00246000
         BE    C01ISBRA                                           75128 00247000
GOTTOKEY   STH R1,TKNCURR                                               00248000
         MVC   DBL(2),C01LAR15 SHIFT AN LA R15,0 INSTRUCTION      75128 00249000
         MVC   DBL+2(2),8(R10) AND THE S(ADDR RTN)                75128 00250000
         EX    R0,DBL         LOAD RTN ADDRESS                    75128 00251000
           LTR R15,R15                                                  00252000
         BZ    C01NIMPL                                           75128 00253000
         MVC   #WORK(L'TKN+2),#TKN SAVE TOKEN AFTER'=('           75311 00254000
           BALR R14,R15                                                 00255000
