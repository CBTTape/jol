SM         GETTKN (R1)         GET NEXT TOKEN                     75311 02240000
           CLI TKN,C'='        '='                                75311 02250000
           BE  SM              YES,SKIP IT                        75311 02260000
           CLI TKN,C'('        '('                                75311 02270000
           BE  SM              YES,SKIP IT                        75311 02280000
* HERE WE HAVE THE THING AFTER THE SM=( KEYWORDS                  75311 02290000
           STH R1,TKNCURR      SAVE TKN POINTER                   75311 02300000
           TNUM #TKN           CONVERT TO NUMERIC                 75311 02310000
           LTR R15,R15         DID IT ?                           75311 02320000
           BNZ SMERR           NO -> GIVE ERROR MESSAGE           75311 02330000
           STH R1,SM1          YES, STORE SM1 FIELD               75311 02340000
           LH  R1,TKNCURR      RE-LOAD TOKEN POINTER.             75311 02350000
P95SM2     GETTKN (R1)         GET NEXT TOKEN                     75311 02360000
           CLI TKN,C','        ','                                75311 02370000
           BE  P95SM2          YES, SKIP IT                       75311 02380000
           STH R1,TKNCURR      NO, SAVE CURRENT TOKEN NUMBER.     75311 02390000
           TNUM #TKN           YES, CONVERT TO NUMBERIC           75311 02400000
           LTR R15,R15         DID IT CONVERT ?                   75311 02410000
           BNZ SMERR           NO, GIVE ERROR                     75311 02420000
           STH R1,SM2          YES, STORE SM2 FIELD               75311 02430000
           LH  R1,TKNCURR      GET NEXT                           75311 02440000
P95SM3     GETTKN (R1)         AND                                75311 02450000
           CLI TKN,C','            SO                             75311 02460000
           BE  P95SM3                 ON                          75311 02470000
           STH R1,TKNCURR                                         75311 02480000
          TNUM #TKN                                               75311 02490000
           LTR R15,R15                                            75311 02500000
           BNZ SMERR                                              75311 02510000
           STH R1,SM3                                             75311 02520000
* NOW CHECK FOR ')'                                               75311 02530000
           GETTKN TKNCURR                                         75311 02540000
           CLI TKN,C')'                                           75311 02550000
           BNE *+8                                                75311 02560000
           STH R1,TKNCURR                                         75311 02570000
* HERE WE HAVE REACHED THE END OF THE SM FIELDS.                  75311 02580000
* WE HAD BETTER PERFORM A VALIDITY CHECK OR TWO.                  75311 02590000
           CLC SM1,SM2                                            75311 02600000
           BL  P95SM1OK                                           75311 02610000
           JOLERR 206,'SM2 NOT GREATER SM1',                      75311*02620000
               ':- DEFAULT SOURCE MARGINS USED'                   75311 02630000
           B   P95SMDEF                                           75311 02640000
* NOW CHECK THAT SM3 IS NOT IN THE MIDDLE OF THE RANGE.           75311 02650000
P95SM1OK   CLC SM3,ZERO        =0 ?                               75311 02660000
           BE  P95SM3OK        OK THEN                            75311 02670000
           CLC SM2,SM3                                            75311 02680000
           BL  P95SM3OK                                           75311 02690000
           JOLERR 207,'SM(3) IS WITHIN SM(1)-SM(2) RANGE',        75311*02700000
               ':-DEFAULT SOURCE MARGINS USED'                    75311 02710000
           B   P95SMDEF                                           75311 02720000
P95SM3OK   B   P95SKIP2        NEXT ITEM ...                      75311 02730000
P95SMDEF   MVC  SM1,=H'1'                                               02740000
           MVC  SM2,=H'72'                                              02750000
           MVC  SM3,=H'73'                                              02760000
