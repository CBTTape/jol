P95SYM     EQU *                                                        06840000
           GETTKN (1)          GET SYMBOL AFTER '/'                     06850000
           STH  R1,TKNCURR                                              06860000
           CLI TKN,C' '        IS IT '',IE NO SYMBOLIC NAMES ?          06870000
         BE    P95RETN0                                           75128 06880000
P95SYM2    EQU *                                                        06890000
           CLI TKN,C')'        IGNORE IT                                06900000
           BE  P95SYM                                                   06910000
           MVC ICOMMAND,=CL8'MACRO' DO THIS SO ASSIGNMENT ROUTINE       06920000
*                              WON'T TRY TO DO ANYTHING FANCY WHEN      06930000
*                              STORING THE VALUES OF THE SYMBOLICS      06940000
           CLI TKNTYPE,2       <IDENTIFIER> ?                           06950000
           BE  P95IDENT                                                 06960000
           CLI TKN,C','                                                 06970000
           BE  P95SKIP3                                                 06980000
         JOLERR 302,'INVALID NAME ''',#TKN,''' IGNORED IN * JOL OR PARA*06990000
               METER FIELD OF JOL COMPILER'                             07000000
P95SKIP3   EQU  *                                                       07010000
           LH  R1,TKNCURR                                               07020000
           B   P95SYM                                                   07030000
           SPACE                                                        07040000
P95IDENT   EQU *                                                        07050000
         MVC   SYMBOLIC,TKN   SHIFT IN SYMBOLIC NAME              75128 07060000
           XC  #WORK,#WORK     NULLIFY ITEM TO BE STORED TEMPORARILY    07070000
           GETTKN (R1)                                                  07080000
           STH R1,TKNCURR                                               07090000
           CLI TKN,C'='                                                 07100000
           BE  P95GOTVL        GOT A VALUE MAYBE,GO AND GET NEXT,       07110000
*                              IF IT IS A COMMA,IGNORE IT.              07120000
           SPACE                                                        07130000
* NOW THE VALUE HERE MUST BE A COMMA,OR IT IS AN ERROR.                 07140000
           CLI TKN,C','                                                 07150000
           BE  P95NOV1                                                  07160000
         JOLERR 303,'INVALID VALUE ''',#TKN,                           *07170000
               ''' IGNORED IN * JOL OR PARAMETER FIELD OF JOL COMPILER' 07180000
           LH  R1,TKNCURR                                               07190000
           B   P95SYM                                                   07200000
P95GOTVL   EQU *                                                        07210000
           GETTKN (1)          GET VALUE AFTER '='                      07220000
           STH R1,TKNCURR                                               07230000
           CLI TKN,C','                                                 07240000
           BE  P95NOV1         NO VALUE,STORE NULL                      07250000
           MVC #WORK(L'TKN+2),#TKN                                      07260000
P95NOV1   $CALL UJP85ASN       STORE NAME IN TABLE                      07270000
           B   P95SYM          GO GET NEXT                              07280000
